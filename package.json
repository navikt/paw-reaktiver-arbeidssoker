{
    "name": "paw-reaktiver-arbeidssoker",
    "version": "1.0.0",
    "description": "Reaktiverer arbeidssøkere som har blitt inaktivert, men som svarer at de fortsatt ønsker å være registrert på meldekortet",
    "main": "index.js",
    "scripts": {
        "build": "tsc",
        "start": "ts-node-dev src/index.ts | pino-pretty",
        "dev": "ts-node-dev -r dotenv/config src/index.ts | pino-pretty",
        "test:ci": "npm run lint",
        "lint": "eslint --ext .ts src/**",
        "prettier": "prettier --write 'src/**/*.ts'",
        "refresh": "rm -rf node_modules && rm package-lock.json && npm i",
        "prepare": "husky install"
    },
    "dependencies": {
        "@elastic/ecs-pino-format": "1.3.0",
        "kafkajs": "2.2.3",
        "openid-client": "5.3.1",
        "pino": "8.7.0",
        "pino-http": "8.2.1",
        "prom-client": "14.1.0",
        "zod": "3.19.1"
    },
    "devDependencies": {
        "@types/node": "18.11.12",
        "@typescript-eslint/eslint-plugin": "5.46.0",
        "@typescript-eslint/parser": "5.46.0",
        "dotenv": "16.0.3",
        "eslint": "8.29.0",
        "husky": "8.0.2",
        "lint-staged": "13.1.0",
        "pino-pretty": "9.1.1",
        "prettier": "2.8.1",
        "pretty-quick": "3.1.3",
        "ts-jest": "29.0.3",
        "ts-node-dev": "2.0.0",
        "typescript": "4.9.4"
    },
    "keywords": [],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/navikt/paw-reaktiver-arbeidssoker.git"
    },
    "prettier": {
        "printWidth": 120,
        "singleQuote": true,
        "tabWidth": 4
    },
    "lint-staged": {
        "*.{js,jsx,json,md,ts,tsx}": "prettier --write"
    },
    "engines": {
        "node": ">=18.12.1"
    },
    "author": "",
    "license": "MIT"
}
